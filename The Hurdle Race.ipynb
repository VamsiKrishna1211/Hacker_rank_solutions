    "def hurdleRace(k,h):\n",
    "    mx=max(h)\n",
    "    if(mx-k<0):\n",
    "        return 0\n",
    "    else:\n",
    "        return (mx-k)\n",
    "    \n",
    "    \n",
    "if __name__=='__main__':\n",
    "    n,k=map(int, input().rstrip().split())\n",
    "    h=list(map(int, input().rstrip().split()))\n",
    "    print(hurdleRace(k,h))"
   ]
