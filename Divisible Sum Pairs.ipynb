   "source": [
    "def divisibleSumPairs(n, k, ar):\n",
    "    count = 0\n",
    "    for j in range(n):\n",
    "        for i in range(j):\n",
    "            if((ar[j]+ar[i])%k==0 and i<j):\n",
    "                count+=1\n",
    "    return(count)"
   ]
  },
